#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    unsigned long long int T, N, i, j, prime;
	cin >> T;
	for (i = 0; i<T; i++) {
		
		cin >> N;
		prime = N;
		for (j = 2; j < N; j++) {
			if (N%j == 0) prime = j;
		}

		cout << prime << endl;
		
	}
    return 0;
}
